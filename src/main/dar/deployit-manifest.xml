<?xml version="1.0" encoding="UTF-8"?>
<udm.DeploymentPackage version="${noSnapshot(project.version)}" application="XL Release GitHub Plugin">
    <deployables>
        <xlrelease.Plugin
                name="${project.rootProject.name}-${project.version}.jar"
                file="${artifact(project.jar)}">
            <scanPlaceholders>false</scanPlaceholders>
        </xlrelease.Plugin>
    </deployables>
</udm.DeploymentPackage>